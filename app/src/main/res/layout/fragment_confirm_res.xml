<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/cornor_rectangle"
    tools:context=".facility.confirmResFragment">

    <ImageView
        android:id="@+id/btn_x"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="281dp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="17dp"
        android:layout_marginBottom="174dp"
        android:src="@android:drawable/ic_menu_close_clear_cancel"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="83dp"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="83dp"
        android:layout_marginBottom="127dp"
        android:text="예약을 확정하시겠습니까?"
        android:textColor="#000000"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="82dp"
        android:layout_marginTop="95dp"
        android:layout_marginEnd="78dp"
        android:layout_marginBottom="86dp"
        android:text="(도서관 C1 14:00 ~ 15:00)"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/view_cen"
        android:layout_width="156dp"
        android:layout_height="56dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="30dp"
        android:src="@drawable/image_for_blue"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView27" />

    <ImageView
        android:id="@+id/view_ok"
        android:layout_width="156dp"
        android:layout_height="56dp"
        android:src="@drawable/image_for"
        app:layout_constraintBottom_toBottomOf="@+id/view_cen"
        app:layout_constraintStart_toEndOf="@+id/view_cen" />

    <TextView
        android:id="@+id/textView28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="취소"
        android:textColor="#FFFFFF"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/view_cen"
        app:layout_constraintEnd_toEndOf="@+id/view_cen"
        app:layout_constraintStart_toStartOf="@+id/view_cen"
        app:layout_constraintTop_toTopOf="@+id/view_cen" />

    <TextView
        android:id="@+id/textView29"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="확정"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/view_ok"
        app:layout_constraintEnd_toEndOf="@+id/view_ok"
        app:layout_constraintStart_toStartOf="@+id/view_ok"
        app:layout_constraintTop_toTopOf="@+id/view_ok" />

</androidx.constraintlayout.widget.ConstraintLayout>